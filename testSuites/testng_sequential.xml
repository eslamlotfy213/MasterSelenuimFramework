<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite parallel="none"  name="Suite" thread-count="2">

  <listeners>
    <listener class-name="baseComponent.Listeners"/>
  </listeners>


  <test  name="Submit Order Test">
    <classes>
      <class name="Selenium.tests.SubmitOrderTest"/>
    </classes>
  </test> <!-- Test -->

  <test  name="Error Validations Test">
    <classes>
      <class name="Selenium.tests.ErrorValidationTest"/>
    </classes>
  </test>

  <test  name="View Order Test">
    <classes>
      <class name="Selenium.tests.ViewOrderTest"/>
    </classes>
  </test> <!-- Test -->

</suite> <!-- Suite -->


